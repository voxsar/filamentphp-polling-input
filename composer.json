{
    "name": "voxsar/poll-url-input",
    "description": "A Filament custom input that polls a URL and updates its value.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vendor\\PollUrlInput\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "laravel/framework": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voxsar\\PollUrlInput\\PollUrlInputServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
